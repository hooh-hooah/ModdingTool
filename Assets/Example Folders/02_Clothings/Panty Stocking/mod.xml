<packer>
    <!-- This will be copied as manifest.xml -->
    <guid>modbuilder.example.panst</guid>
    <name>MODBUILDER PANTYSTOCKING EXAMPLE</name>
    <version>1.0.0</version>
    <author>hooh</author>
    <description>This is modbuilder example.</description>

    <!-- This section will contain bundle information -->
    <bundles>
        <folder auto-path="prefabs" from="prefabs"/>
        <folder auto-path="thumbs" from="thumbs"/>
    </bundles>

    <!-- This section will contain build information -->
    <build>
        <list type="fpanst">
            <item
                name="@@EXAMPLE PANTYSTOCKING" mesh-a="clothmesh" tex-main="diffuse" tex-mask="colormask" tex-main2="0" tex-mask2="0" hide-bottom="0" thumb="thumb_image"
            />
        </list>
    </build>
</packer>